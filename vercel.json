{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js",
        "methods": ["GET", "POST","PUT","DELETE","PATCH","OPTIONS"],
        "headers":{
          "Acess-Control-Allow-Origin": "*"
        }
      }
    ],
    "crons": [
      {
        "path": "/cronJobTodayArtist",
        "schedule": "18 2 * * *"
      }
    ]
}